  "dependencies": {
    "sass": "^1.25.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0"
  }
}
