{
  "name": "atelier-ludique",
  "version": "1.0.0",
  "description": "brief-projet",
  "main": "index.js",
  "scripts": {
    "watch-sass": "node-sass sass/main.scss css/style.css --watch",
    "compile-sass": "node-sass sass/main.scss css/style.comp.css",
    "concat-css": "concat -o css/style.concat.css css/additional.css css/style.comp.css",
    "compress-css": "node-sass css/style.concat.css css/style.css --output-style compressed",
    "build-css": "npm-run-all compile-sass concat-css compress-css"
  },
